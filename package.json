{
  "name": "auth-service",
  "version": "0.1.0",
  "description": "Boilerplate code for node.js server using express.js, typescript and jest test runner",
  "main": "dist/index.js",
  "author": "Vishnu Singh <vishnusingh1729@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "yarn run build && NODE_ENV=test jest dist --maxWorkers=1",
    "test:watch": "yarn test --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "start:dev": "gulp",
    "build": "gulp clean && gulp build",
    "start": "yarn run build && NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "awilix": "^7.0.3",
    "awilix-express": "^7.0.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.6.0",
    "morgan": "^1.10.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@shelf/jest-mongodb": "^2.2.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-file-cache": "^0.0.1",
    "gulp-nodemon": "^2.5.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^28.1.0",
    "merge": "^2.1.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "typescript": "^4.7.2"
  }
}
